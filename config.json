{
  "repository": {
    "local_path": ".",
    "remote_url": "",
    "branch": "main"
  },
  "commit": {
    "message_templates": [
      "Daily update: {date}",
      "Automated commit: {date}",
      "Daily maintenance: {date}",
      "Progress update: {date}",
      "Daily log entry: {date}"
    ],
    "file_to_update": "daily_log.md",
    "commit_time": "09:00",
    "timezone": "UTC"
  },
  "schedule": {
    "enabled": true,
    "skip_weekends": false,
    "custom_schedule": []
  },
  "logging": {
    "enabled": true,
    "log_file": "automation.log",
    "level": "INFO"
  }
}